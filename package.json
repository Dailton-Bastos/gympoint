{
  "name": "gympoint",
  "version": "0.1.0",
  "private": false,
  "author": {
    "name": "Dailton Bastos",
    "email": "dailtonbastos@gmail.com",
    "url": "https://github.com/Dailton-Bastos/gympoint"
  },
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "type-check": "tsc --project tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "preview-email": "email dev --dir src/components/emails --port 3333",
    "prepare": "husky install",
    "commit": "cz",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} src/lib/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.3.3",
    "@hookform/resolvers": "^3.3.4",
    "@prisma/client": "5.9.1",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@react-email/components": "0.0.14",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.316.0",
    "next": "14.0.4",
    "next-auth": "^5.0.0-beta.9",
    "nodemailer": "^6.9.9",
    "react": "^18",
    "react-dom": "^18",
    "react-email": "2.0.0",
    "react-hook-form": "^7.49.3",
    "react-icons": "^5.0.1",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^14.1.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.11",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "autoprefixer": "^10.0.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-only-warn": "^1.1.0",
    "git-commit-msg-linter": "^5.0.6",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8",
    "prettier": "^3.1.1",
    "prisma": "^5.9.1",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --max-warnings 0 .",
      "prettier --write ."
    ],
    "*.{css,scss,md,js,json}": [
      "prettier --write ."
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
